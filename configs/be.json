{
    "def": {
        "consonant": [
            "б", "в", "г", "ґ", "д", "ж", "з", "й", "к", "л", "м",
            "н", "п", "р", "с", "т", "ф", "х", "ц", "ч", "ш"
        ],
        "vowel": [
            "а", "е", "ё", "і", "о", "у", "ў", "ы", "э", "ю", "я"
        ],
        "nonIotizedVowel": [
            "а", "і", "о", "у", "ы", "э"
        ]
    },
    "ignore": ["\u0301"],
    "map": [
        {"key": "а", "to": "a"},
        {"key": "б", "to": "b"},
        {"key": "в", "to": "v"},
        {"key": "г", "to": "g"},
        {"key": "ґ", "to": "ġ"},
        {"key": "д", "to": "d"},
        {"key": "е", "from": ["#consonant", "е"], "to": ["~", "e"]},
        {"key": "е", "to": "je"},
        {"key": "ё", "from": ["#consonant", "ё"], "to": ["~", "io"]},
        {"key": "ё", "to": "jo"},
        {"key": "ж", "to": "ž"},
        {"key": "з", "to": "z"},
        {"key": "і", "from": ["і", "й", "NOT #vowel"], "to": ["í", "", "~"]},
        {"key": "і", "from": ["і", ["а", "о", "у", "э"]], "to": ["í", "~"]},
        {"key": "і", "from": ["OTHER #vowel", "і"], "to": ["~", "í"]},
        {"key": "і", "to": "i"},
        {"key": "й", "from": ["#vowel", "й", "NOT #vowel"], "to": ["~", "i", "~"]},
        {"key": "й", "to": "j"},
        {"key": "к", "to": "k"},
        {"key": "л", "to": "l"},
        {"key": "м", "to": "m"},
        {"key": "н", "to": "n"},
        {"key": "о", "to": "o"},
        {"key": "п", "to": "p"},
        {"key": "р", "to": "r"},
        {"key": "с", "to": "s"},
        {"key": "т", "to": "t"},
        {"key": "у", "to": "u"},
        {"key": "ў", "to": "w"},
        {"key": "ф", "to": "f"},
        {"key": "х", "to": "h"},
        {"key": "ц", "to": "c"},
        {"key": "ч", "to": "č"},
        {"key": "ш", "to": "š"},
        {"key": "ы", "from": ["ы", "й", "NOT #vowel"], "to": ["ý", "", "~"]},
        {"key": "ы", "from": ["ы", ["а", "о", "у", "э"]], "to": ["ý", "~"]},
        {"key": "ы", "from": ["OTHER #vowel", "ы"], "to": ["~", "ý"]},
        {"key": "ы", "to": "y"},
        {"key": "э", "from": ["#consonant", "э"], "to": ["~", "é"]},
        {"key": "э", "to": "e"},
        {"key": "ю", "from": ["#consonant", "ю"], "to": ["~", "iu"]},
        {"key": "ю", "to": "ju"},
        {"key": "я", "from": ["#consonant", "я"], "to": ["~", "ia"]},
        {"key": "я", "to": "ja"},

        {"key": "ь", "from": [["ж", "ч", "ш"], "ь", ["і", "о"]], "to": ["~", "j", "~"]},
        {"key": "ь", "from": ["г", "ь", ["і", "о"]], "to": ["ǵ", "j", "~"]},
        {"key": "ь", "from": ["з", "ь", ["і", "о"]], "to": ["ź", "j", "~"]},
        {"key": "ь", "from": ["к", "ь", ["і", "о"]], "to": ["ḱ", "j", "~"]},
        {"key": "ь", "from": ["л", "ь", ["і", "о"]], "to": ["ĺ", "j", "~"]},
        {"key": "ь", "from": ["м", "ь", ["і", "о"]], "to": ["ḿ", "j", "~"]},
        {"key": "ь", "from": ["н", "ь", ["і", "о"]], "to": ["ń", "j", "~"]},
        {"key": "ь", "from": ["п", "ь", ["і", "о"]], "to": ["ṕ", "j", "~"]},
        {"key": "ь", "from": ["р", "ь", ["і", "о"]], "to": ["ŕ", "j", "~"]},
        {"key": "ь", "from": ["с", "ь", ["і", "о"]], "to": ["ś", "j", "~"]},
        {"key": "ь", "from": ["ц", "ь", ["і", "о"]], "to": ["ć", "j", "~"]},
        {"key": "ь", "from": ["#consonant", "ь", ["і", "о"]], "to": ["~", "\u0301j", "~"]},
        {"key": "ь", "from": [["ж", "ч", "ш"], "ь", ["а", "у", "ы", "э"]], "to": ["~", "-", "~"]},
        {"key": "ь", "from": ["г", "ь", ["а", "у", "ы", "э"]], "to": ["ǵ", "-", "~"]},
        {"key": "ь", "from": ["з", "ь", ["а", "у", "ы", "э"]], "to": ["ź", "-", "~"]},
        {"key": "ь", "from": ["к", "ь", ["а", "у", "ы", "э"]], "to": ["ḱ", "-", "~"]},
        {"key": "ь", "from": ["л", "ь", ["а", "у", "ы", "э"]], "to": ["ĺ", "-", "~"]},
        {"key": "ь", "from": ["м", "ь", ["а", "у", "ы", "э"]], "to": ["ḿ", "-", "~"]},
        {"key": "ь", "from": ["н", "ь", ["а", "у", "ы", "э"]], "to": ["ń", "-", "~"]},
        {"key": "ь", "from": ["п", "ь", ["а", "у", "ы", "э"]], "to": ["ṕ", "-", "~"]},
        {"key": "ь", "from": ["р", "ь", ["а", "у", "ы", "э"]], "to": ["ŕ", "-", "~"]},
        {"key": "ь", "from": ["с", "ь", ["а", "у", "ы", "э"]], "to": ["ś", "-", "~"]},
        {"key": "ь", "from": ["ц", "ь", ["а", "у", "ы", "э"]], "to": ["ć", "-", "~"]},
        {"key": "ь", "from": ["#consonant", "ь", ["а", "у", "ы", "э"]], "to": ["~", "\u0301-", "~"]},
        {"key": "ь", "from": [["ж", "ч", "ш"], "ь"], "to": ["~", ""]},
        {"key": "ь", "from": ["г", "ь"], "to": ["ǵ", ""]},
        {"key": "ь", "from": ["з", "ь"], "to": ["ź", ""]},
        {"key": "ь", "from": ["к", "ь"], "to": ["ḱ", ""]},
        {"key": "ь", "from": ["л", "ь"], "to": ["ĺ", ""]},
        {"key": "ь", "from": ["м", "ь"], "to": ["ḿ", ""]},
        {"key": "ь", "from": ["н", "ь"], "to": ["ń", ""]},
        {"key": "ь", "from": ["п", "ь"], "to": ["ṕ", ""]},
        {"key": "ь", "from": ["р", "ь"], "to": ["ŕ", ""]},
        {"key": "ь", "from": ["с", "ь"], "to": ["ś", ""]},
        {"key": "ь", "from": ["ц", "ь"], "to": ["ć", ""]},
        {"key": "ь", "from": ["#consonant", "ь"], "to": ["~", "\u0301"]},
        {"key": "ь", "to": ""},

        {"key": "'", "from": ["'", "#nonIotizedVowel"], "to": ["j", "~"]},
        {"key": "'", "to": ""},
        {"key": "ʼ", "from": ["ʼ", "#nonIotizedVowel"], "to": ["j", "~"]},
        {"key": "ʼ", "to": ""}
    ]
}